{
  "name": "@powerhousedao/notes-demo-package",
  "description": "",
  "version": "1.0.4",
  "license": "AGPL-3.0-only",
  "type": "module",
  "private": false,
  "files": [
    "/dist"
  ],
  "exports": {
    ".": {
      "source": "./index.ts",
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./document-models": {
      "types": "./dist/document-models/index.d.ts",
      "default": "./dist/document-models/index.js"
    },
    "./editors": {
      "types": "./dist/editors/index.d.ts",
      "default": "./dist/editors/index.js"
    },
    "./document-models/*": {
      "types": "./dist/document-models/*/index.d.ts",
      "default": "./dist/document-models/*/index.js"
    },
    "./editors/*": {
      "types": "./dist/editors/*/index.d.ts",
      "default": "./dist/editors/*/index.js"
    },
    "./subgraphs": {
      "types": "./dist/subgraphs/index.d.ts",
      "default": "./dist/subgraphs/index.js"
    },
    "./processors": {
      "types": "./dist/processors/index.d.ts",
      "default": "./dist/processors/index.js"
    },
    "./manifest": {
      "default": "./dist/powerhouse.manifest.json"
    },
    "./style.css": "./dist/style.css"
  },
  "scripts": {
    "build": "npm run tsc && npm run tailwind",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint --config eslint.config.js --cache --cache-strategy content",
    "lint:fix": "npm run lint --fix",
    "tsc": "tsc",
    "tsc:watch": "tsc --watch",
    "tailwind": "npx @tailwindcss/cli -i ./style.css -o ./dist/style.css",
    "prepublishOnly": "npm run build",
    "check-circular-imports": "npx dpdm -T ./index.ts",
    "generate": "ph-cli generate",
    "connect": "ph-cli connect",
    "reactor": "ph-cli reactor",
    "service": "ph-cli service",
    "vetra": "ph-cli vetra",
    "service-startup": "bash ./node_modules/@powerhousedao/ph-cli/dist/scripts/service-startup.sh",
    "service-unstartup": "bash ./node_modules/@powerhousedao/ph-cli/dist/scripts/service-unstartup.sh"
  },
  "dependencies": {
    "@powerhousedao/builder-tools": "5.3.0-staging.5",
    "@powerhousedao/common": "5.3.0-staging.5",
    "@powerhousedao/design-system": "5.3.0-staging.5",
    "@powerhousedao/document-engineering": "^1.39.0",
    "@powerhousedao/vetra": "5.3.0-staging.5",
    "document-model": "5.3.0-staging.5",
    "graphql": "^16.10.0",
    "graphql-tag": "^2.12.6",
    "luxon": "^3.7.2",
    "uuid": "^11.1.0",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@openfeature/core": "^1.9.1",
    "@electric-sql/pglite": "^0.2.17",
    "@eslint/js": "^9.38.0",
    "@powerhousedao/analytics-engine-core": "^0.5.0",
    "@powerhousedao/codegen": "5.3.0-staging.5",
    "@powerhousedao/config": "5.3.0-staging.5",
    "@powerhousedao/connect": "5.3.0-staging.5",
    "@powerhousedao/ph-cli": "5.3.0-staging.5",
    "@powerhousedao/reactor-api": "5.3.0-staging.5",
    "@powerhousedao/reactor-browser": "5.3.0-staging.5",
    "@powerhousedao/reactor-local": "5.3.0-staging.5",
    "@powerhousedao/scalars": "staging",
    "@powerhousedao/switchboard": "5.3.0-staging.5",
    "@tailwindcss/cli": "^4.1.4",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^24.9.2",
    "@types/react": "^19.2.2",
    "@vitejs/plugin-react": "^5.1.0",
    "document-drive": "5.3.0-staging.5",
    "eslint": "^9.38.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "globals": "^16.4.0",
    "package-manager-detector": "^0.2.8",
    "pm2": "^5.4.3",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "tailwindcss": "^4.1.16",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.2",
    "vite": "^6.2.3",
    "vite-plugin-node-polyfills": "^0.24.0",
    "vitest": "^3.0.9"
  },
  "peerDependencies": {
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "overrides": {
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  }
}
